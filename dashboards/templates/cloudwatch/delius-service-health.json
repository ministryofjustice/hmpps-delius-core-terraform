{
  "start": "-PT6H",
  "widgets": [
    {
      "height": 3,
      "width": 9,
      "y": 8,
      "x": 6,
      "type": "metric",
      "properties": {
        "metrics": [
          [ "AWS/EC2", "CPUUtilization", "InstanceId", "${instance_delius_db_1}", { "label": "Database" } ],
          [ "...", "AutoScalingGroupName", "${asg_ldap}", { "label": "LDAP", "stat": "Maximum" } ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${region}",
        "period": 300,
        "title": "CPU Load",
        "legend": {
          "position": "right"
        },
        "liveData": true
      }
    },
    {
      "height": 6,
      "width": 6,
      "y": 8,
      "x": 0,
      "type": "alarm",
      "properties": {
        "title": "Instance Status",
        "alarms": [
          "${alarm_ldap}",
          "${alarm_activemq}",
          "${alarm_weblogic_interface}",
          "${alarm_weblogic_ndelius}",
          "${alarm_weblogic_spg}"
        ]
      }
    },
    {
      "height": 1,
      "width": 15,
      "y": 7,
      "x": 0,
      "type": "text",
      "properties": {
        "markdown": "\n# Instance Health\n"
      }
    },
    {
      "height": 1,
      "width": 9,
      "y": 7,
      "x": 15,
      "type": "text",
      "properties": {
        "markdown": "\n# Application Health\n"
      }
    },
    {
      "height": 1,
      "width": 12,
      "y": 0,
      "x": 0,
      "type": "text",
      "properties": {
        "markdown": "\n# User Activity\n"
      }
    },
    {
      "height": 1,
      "width": 12,
      "y": 0,
      "x": 12,
      "type": "text",
      "properties": {
        "markdown": "\n# SPG Activity\n"
      }
    }
  ]
}